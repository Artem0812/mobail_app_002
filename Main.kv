
<Entrance>:
    button:button
    button2:button2
    padding:5
    Image:
        source: 'космос.webp'
        size_hint_x: 3.4
        size_hint_y: 1.3
        allow_stretch: 2
        pos: -10, -10
    FloatLayout:
        pos_hint: {'center_x': 0.5, 'center_y': 0.705}
        size_hint_y: 0.7
        Button:
            id: button
            text: "Регистрация"
            pos: 120, 600
            size_hint: 0.5, 0.13
            background_normal: '' # Опустошаем нормальное изображение фона
            background_down: '' # Опустошаем изображение фона при нажатии
            background_color: (1, 1, 1, 0) # Установка альфа-канала равным 0
            border: (2, 2, 2, 2) # Задание ширины границы
            border_color: (0, 0, 0, 1) # Задание цвета границы
            on_press: root.first_main()
        Button:
            id: button2
            text: "Вход"
            pos: 120, 300
            size_hint: 0.5, 0.13
            background_normal: '' # Опустошаем нормальное изображение фона
            background_down: '' # Опустошаем изображение фона при нажатии
            background_color: (1, 1, 1, 0) # Установка альфа-канала равным 0
            border: (2, 2, 2, 2) # Задание ширины границы
            border_color: (0, 0, 0, 1) # Задание цвета границы
            on_press: root.Auth()

<Authorization>:
    but:but
    nam:nam
    passwd:passwd
    label:label

    padding:5

    Image:
        source: 'космос.webp'
