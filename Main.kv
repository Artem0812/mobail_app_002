            on_release:
                app.root.current = "first"
                root.manager.transition.direction = "right"

    GridLayout:
        cols: 2
        row_default_height: '100dp'
        row_force_default: True
        size_hint_y: 0.7





        Button:
            id: but1
            text: "Почему так\n шея затекает?"
