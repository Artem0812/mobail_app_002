        size_hint_x: 3.5
        size_hint_y: 1.3
        allow_stretch: 2
        pos: -600, 1


    FloatLayout:
        pos_hint: {'center_x': 0.5, 'center_y': 0.705}
        size_hint_y: 0.7



        Button:

            id: but
            text: "Продолжить"
            background_normal: '' # Опустошаем нормальное изображение фона
            background_down: '' # Опустошаем изображение фона при нажатии
            background_color: (1, 1, 1, 0) # Установка альфа-канала равным 0
            border: (2, 2, 2, 2) # Задание ширины границы
            border_color: (0, 0, 0, 1) # Задание цвета границы
            size_hint: 1, 0.15
            on_press: root.ladel_function_text()
#

    Label:
        id: label_text
        text: "Авторизация"
        pos: 5, 670
        color: 173, 207, 139
        size_hint: 1, 0.15
        font_size: 24

    Label:
        id: label
        text: ""
        pos: 30, 430
        color: 173, 207, 139
        size_hint: 1, 0.15
        font_size: 24

    FloatLayout:

        GridLayout:
            cols: 2
            font_size: 24
            row_default_height: '50dp'
            row_force_default: True
            size_hint: 0.7, 0.5
            pos: 50, 220
            text_size: 24





            Label:
                text: "Name: "
                size_hint: 0, 0.5
                color: 173, 207, 139
                font_size: 24



            TextInput:
                id: nam
                multiline: False
                size_hint_x: 0.1



            Label:
                text: "Password: "
                size_hint_x: None
                color: 173, 207, 139
                font_size: 24


            TextInput:
                id: passwd
                password: True
                multiline: False
                size_hint_x: 0.1

